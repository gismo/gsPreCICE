{
  "participant": {
    "name": "Solid",
    "preciceConfigFile": "precice-config.xml"
  },
  "geometry": {
    "type": "rectangle",
    "bounds": {
      "x0": 0.0,
      "y0": 0.0,
      "x1": 0.5,
      "y1": 1.0
    }
  },
  "refinement": {
    "numElevate": 0,
    "numRefine": 1
  },
  "material": {
    "density": 3000.0,
    "youngsModulus": 4e6,
    "poissonsRatio": 0.3,
    "thickness": 0.1
  },
  "boundaryConditions": [
    {
      "patch": 0,
      "boundary": "south",
      "type": "dirichlet"
    },
    {
      "patch": 0,
      "boundary": "south",
      "type": "clamped",
      "component": 2
    }
  ],
  "solver": {
    "method": "newmark",
    "tolerance": 1e-3,
    "verbose": true
  },
  "output": {
    "directory": "./output",
    "plot": true,
    "plotmod": 1,
    "pointData": {
      "enabled": true,
      "filename": "./output/solidPointData.csv",
      "points": {
        "rows": 1,
        "cols": 2,
        "data": [0.5, 1.0]
      }
    }
  }
} 